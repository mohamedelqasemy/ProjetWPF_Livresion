<Window x:Class="AppGestionEtudiant.Gestion_filiere"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppGestionEtudiant"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        mc:Ignorable="d"
        Title="Gestion_filiere" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="1.3*" />
        </Grid.RowDefinitions>

        <telerik:RadCarousel Width="400" Height="200">
            <telerik:RadCarousel.Items>
                <telerik:CarouselItem Content="Item 1" />
                <telerik:CarouselItem Content="Item 2" />
                <telerik:CarouselItem Content="Item 3" />
            </telerik:RadCarousel.Items>
        </telerik:RadCarousel>




    </Grid>
</Window>
